<ul class="card-set">
	    {{#each this}}
	    <li class="card-item">

	        <div class="card">	

	            <div class="card-img">
	                <a class="card-link" href="{{largeImageURL}}">
	                    <img src="{{webformatURL}}" alt="{{tags}}" loading="lazy" />
	                </a>
	            </div>
	            <div class="card-info">
	                <p class="info-item">
	                    <span>
	                        <i class="fa-solid fa-heart fa-xl"></i>
	                    </span>
	                    <b>{{likes}}</b>
	                </p> 
	                <p class="info-item">
	                    <span>
	                        <i class="fa-solid fa-eye fa-xl"></i>
	                    </span>
	                    <b>{{views}}</b>
	                </p>
	                <p class="info-item">
	                    <span>
	                        <i class="fa-solid fa-comment fa-xl"></i>
	                    </span>
	                    <b>{{comments}}</b>
	                </p>
	                <p class="info-item">
	                    <span>
	                        <i class="fa-solid fa-cloud-arrow-down fa-xl"></i>
	                    </span>
	                    <b>{{downloads}}</b>
	                </p>
	            </div>

	        </div>

	    </li>
	    {{/each}}
</ul>

